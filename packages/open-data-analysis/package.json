{
  "name": "open-data-analysis",
  "version": "1.0.0",
  "description": "",
  "keywords": [],
  "license": "ISC",
  "author": "",
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    },
    "./utils": {
      "types": "./dist/utils/index.d.ts",
      "import": "./dist/utils/index.js"
    },
    "./tools/HubCodeInterpreter": {
      "types": "./dist/tools/HubCodeInterpreter.d.ts",
      "import": "./dist/tools/HubCodeInterpreter.js"
    },
    "./tools/ServerCodeInterpreter": {
      "types": "./dist/tools/ServerCodeInterpreter.d.ts",
      "import": "./dist/tools/ServerCodeInterpreter.js"
    }
  },
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "scripts": {
    "build": "concurrently --names \"ESM,CJS,TSC\" -c \"yellow.bold,white.bold,blue.bold\" \"pnpm:build:esm\" \"pnpm:build:cjs\" \"pnpm:build:tsc\"",
    "build:cjs": "pnpm ts-node --esm ../../scripts/esbuild.config.mts --lib --cjs",
    "build:dev": "concurrently --names \"ESM,CJS,TSC\" -c \"yellow.bold,white.bold,blue.bold\" \"pnpm:build:esm --dev\" \"pnpm:build:cjs --dev\" \"pnpm:build:tsc\"",
    "build:esm": "pnpm ts-node --esm ../../scripts/esbuild.config.mts --lib",
    "build:tsc": "tsc --emitDeclarationOnly"
  },
  "dependencies": {
    "@jupyterlab/nbformat": "4.1.0-alpha.4",
    "@jupyterlab/services": "^7.0.9",
    "axios": "^1.6.2",
    "eventsource": "^2.0.2",
    "langchain": "^0.0.199"
  },
  "devDependencies": {
    "@types/eventsource": "^1.1.15",
    "@types/node": "^20.10.1",
    "concurrently": "^8.2.2",
    "dotenv-cli": "^7.3.0",
    "esbuild": "^0.19.8",
    "glob": "^10.3.10",
    "ts-node": "^10.9.1",
    "typescript": "^5.3.2"
  },
  "engines": {
    "node": ">=18"
  }
}
