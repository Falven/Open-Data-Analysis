  "version": "0.2.0",
  "configurations": [
    {
      "name": "Run Example",
      "request": "launch",
      "cwd": "${workspaceFolder}/packages/examples",
      "runtimeExecutable": "pnpm",
      "runtimeArgs": ["--filter", "examples", "start"],
      "skipFiles": ["<node_internals>/**"],
      "type": "node",
      "outputCapture": "std",
      "console": "integratedTerminal",
      "preLaunchTask": "npm: build:dev - packages/open-data-analysis"
    }
  ]
}
