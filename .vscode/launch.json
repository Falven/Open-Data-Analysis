  "version": "0.2.0",
  "configurations": [
    {
      "name": "Run Jupyter Server Example",
      "request": "launch",
      "runtimeArgs": ["start:server"],
      "runtimeExecutable": "pnpm",
      "skipFiles": ["<node_internals>/**"],
      "type": "node",
      "outputCapture": "std",
      "console": "integratedTerminal"
    },
    {
      "name": "Run JupyterHub Example",
      "request": "launch",
      "runtimeArgs": ["start:hub"],
      "runtimeExecutable": "pnpm",
      "skipFiles": ["<node_internals>/**"],
      "type": "node",
      "outputCapture": "std",
      "console": "integratedTerminal"
    }
  ]
}
